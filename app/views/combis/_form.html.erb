<%= simple_form_for @combi do |f| %>
          <%= f.input :model, placeholder: 'Volswagen classic, california..' %>
          <%= f.input :description %>
          <%= f.input :address %>
          <%= f.input :price %>
          <%= f.input :photo, placeholder: 'Upload nice photo of your combi' %>
          <%= f.error_notification message: 'Something went wrong' %>
          <%= f.input :photo_cache, as: :hidden %>
          <%= f.submit class: 'btn btn-success' %>
        <% end %>
